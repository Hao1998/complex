{
  "AWSEBDockerrunVersion": 1,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "142122/complex-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "142122/complex-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "client",
      "image": "142122/complex-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "142122/complex-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMapping": [
        {
        "hostPort": 80,
        "containerPort": 80
        }
      ],
      "links": ["client", "server"]
    }
  ]
}
